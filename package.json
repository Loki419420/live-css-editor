  "devDependencies": {
    "archiver": "^5.3.0",
    "chalk": "^4.1.0",
    "copy-files-from-to": "^3.2.0",
    "cp-file": "^9.0.0",
    "del": "^6.0.0",
    "eslint": "^7.24.0",
    "git-hooks": "^1.1.10",
